#include <iostream>
using namespace std;

class countingSheep{
private:
    int static c;
    bool* a;
    int orig, lastNum;
public:
    countingSheep(int orig):orig(orig), lastNum(0){
        a = new bool[10];
        for(int i=0;i<10;i++)
            a[i] = false;
        c = 0;
    }
    bool isAsleep(){
        for(int i = 0; i<10; i++){
            if(a[i] == false)
                return false;
        }
        return true;
    }
    void updateDigit(int num){
        while(num != 0){
            a[num%10] = true;
            num = num/10;
        }
    }
    void counting(){
        int temp = orig;
        lastNum = temp;
        c++;
        if(orig == 0){
            cout << "Case #" << c << ": INSOMNIA" << '\n';
            return;
        }
        else{
            while(!isAsleep()){
            updateDigit(temp);
            lastNum = temp;
            temp = temp + orig;
            }
            cout << "Case #" << c << ": " << lastNum << '\n';

        }
    }
};
int main(int argc, const char * argv[]) {
    
//    for(int i=0;i<n;i++){
//        countingsheep(
//    }
    countingSheep a(0);
    a.counting();
    
    return 0;
}
